<%= form_with model: @task do |f| %>
  <%= f.label :title, {class: "title_class"} %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed? %>
  <%= f.check_box :completed %>
  <%= f.submit %>
<% end %>
<p></p>
<%= link_to "Back", tasks_path %>
